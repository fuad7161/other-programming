char a[100][100];
bool v[100][100];
int n,m,sum;

void dfs(int x,int y)
{
	v[x][y] = true;
	sum++;
	if(x-1>=0 && a[x-1][y]!='#' && !v[x-1][y]){
		dfs(x-1,y);
	}
	if(x<m-1 && a[x+1][y]!='#' && !v[x+1][y]){
		dfs(x+1,y);
	}
	if(y-1>=0 && a[x][y-1]!='#' && !v[x][y-1]){
		dfs(x,y-1);
	}
	if(y<n-1 && a[x][y+1]!='#' && !v[x][y+1]){
		dfs(x,y+1);
	}
}

void zero()
{
	for(int i=0;i<100;i++){
		for(int j=0;j<100;j++){
			a[i][j] = 0;
			v[i][j] = false;
		}
	}
}

int t=1;
int solve()
{
	happy coding
	zero();
	cin>>n>>m;
	int x,y;
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			cin>>a[i][j];
			if(a[i][j]=='@'){
				x = i; y = j;
			}
		}
	}
	 sum = 0;
	dfs(x,y);
	printf("Case %d: %d\n",t++,sum );
	return 0;
}

int main()
{
    int test = 1, tc = 0;
    scanf("%d", &test);
    //(void)getchar();
    //cin >> test;
    while (test--) {
        //printf("Case %d: ", ++tc);
        solve();
    }
 
    return 0;
}
