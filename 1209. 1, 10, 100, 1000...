const int mod           = 1e9 + 7;
const int inf           = (int)2e9 + 5;
const ll  Inf           = (ll)1e18 + 5;
const int N             = 1e7 + 5;
const int nn           = 3e5 + 5;

int siv[2000];
int siv1[200];

vi v;

int solve()
{
	//happy coding
	
	ll n = cinl();
	
	ll x = (4*(n-1)*2)+1;
	ll y = sqrt(x);
	if(y*y==x){
		v.pb(1);
	}else{
		v.pb(0);
	}

	//error();
	return 0;
}
 
int main()
{
    int test = 1, tc = 0;
    scanf("%d", &test);//(void)getchar();
    while (test--)solve();
    for(auto i: v){
    	cout<<i<<" ";
    }nl;
    return 0;
}
