int solve() 
{
	//happy coding

	string s;
	cin>>s;
	char  c = '7';
	bool g = false;
	for(int i=0;i<s.size();i++){
		if(s[i]=='7') g = true;
	}
	if(g){
		Yes
	}else{
		No;
	}

	return 0;
}
int main(){
