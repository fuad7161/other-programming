const int mod           = 1e9 + 7;
const int inf           = (int)2e9 + 5;
const ll  Inf           = (ll)2e18 + 5;
const int N             = 4e5 + 5;
const int N1            = 1e5 + 5;
int n;
void dfs(string s,char a){
	if(s.size()==n){
		cout<<s<<endl;
		return ;
	}
	for(char c = 'a';c<=a+1;c++){
		string t = s+c;
		dfs(t,max(c,a));
	}
}
 
int solve() 
{
	//happy coding
	cin>>n;
	dfs("",'a'-1);

	//error();
}

int main(){
	
	int test = 1, tc = 0;
	//scanf("%d", &test);//(void)getchar();
	while (test--){
		solve();
	}


	return 0;
}
